weekstart=monday
regex=yes
dateformat=D b Y
dateformat.edit=D.M.Y H:N:S
dateformat.info=D.M.Y H:N:S

uda.githubbody.label=Github Body
uda.githubbody.type=string
uda.githubcreatedon.label=Github Created
uda.githubcreatedon.type=date
uda.githubmilestone.label=Github Milestone
uda.githubmilestone.type=numeric
uda.githubnumber.label=Github Issue/PR #
uda.githubnumber.type=numeric
uda.githubtitle.label=Github Title
uda.githubtitle.type=string
uda.githubtype.label=Github Type
uda.githubtype.type=string
uda.githubupdatedat.label=Github Updated
uda.githubupdatedat.type=date
uda.githuburl.label=Github URL
uda.githuburl.type=string
uda.jiradescription.label=Jira Description
uda.jiradescription.type=string
uda.jiraid.label=Jira Issue ID
uda.jiraid.type=string
uda.jirasummary.label=Jira Summary
uda.jirasummary.type=string
uda.jiraurl.label=Jira URL
uda.jiraurl.type=string

report.github.description=GitHub Issues
report.github.columns=id,project,githubcreatedon,description.combined,githuburl
report.github.labels=ID,Repository,Date,Description,URL
report.github.filter=status:pending +github
report.github.sort=project+,githubcreatedon-,githubnumber+

report.jira.description=Current Working Jira
report.jira.columns=id,project,priority,jiraid,jirasummary,jiraurl
report.jira.labels=ID,Project,Priority,JiraID,Description,URL
report.jira.filter=status:pending +work +jira
report.jira.sort=project+,priority-,entry-

report.work.description=Current Work Tasks
report.work.columns=id,project,priority,entry,description.combined
report.work.labels=ID,Project,Priority,Date,Description
report.work.filter=status:pending +work
report.work.sort=project+,priority-,entry-

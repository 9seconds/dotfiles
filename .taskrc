weekstart=monday
regex=yes
dateformat=D b Y
dateformat.edit=D.M.Y H:N:S
dateformat.info=D.M.Y H:N:S

report.github.description=My GitHub Issues
report.github.columns=id,project,priority,entry,description.combined
report.github.labels=ID,Repository,Priority,Date,Description
report.github.filter=status:pending +github
report.github.sort=project+,priority-,entry-

report.work_jira.description=Current Working Jira
report.work_jira.columns=id,project,priority,entry,description.combined
report.work_jira.labels=ID,Project,Priority,Date,Description
report.work_jira.filter=status:pending +work +jira
report.work_jira.sort=project+,priority-,entry-

report.work.description=Current Work Tasks
report.work.columns=id,project,priority,entry,description.combined
report.work.labels=ID,Project,Priority,Date,Description
report.work.filter=status:pending +work
report.work.sort=project+,priority-,entry-
